<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >
<web-app>
  <display-name>谷客搜</display-name>
   <welcome-file-list>
       <welcome-file>index.html</welcome-file>
   </welcome-file-list>

    <servlet>
        <servlet-name>mainServlet</servlet-name>
        <servlet-class>org.firelion.servlet.MainServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>mainServlet</servlet-name>
        <url-pattern>/url</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>aclkServlet</servlet-name>
        <servlet-class>org.firelion.servlet.AclkServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>aclkServlet</servlet-name>
        <url-pattern>/aclk</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>searchServlet</servlet-name>
        <servlet-class>org.firelion.servlet.SearchServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>searchServlet</servlet-name>
        <url-pattern>/search</url-pattern>
    </servlet-mapping>

</web-app>
